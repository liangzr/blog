figure.highlight
  margin 0 -30px 16px
  padding 0 30px
  background #000
  @media screen and (max-width:580px)
    margin 0 -20px 16px
    padding 0 20px

  pre
    margin 0 -30px 0px
    @media screen and (max-width:580px)
      margin 0 -20px 0px

  table
    margin 0
    border-spacing 0

  td
    padding 0
    border-bottom 0

  .gutter
    width 0px
    overflow: hidden
    @media screen and (max-width:580px)
      display none
    pre
      position relative
      padding 10px 0 10px 30px
      border-right 1px solid #fff
      height 100%
      width 30px

pre
  // Theme: Solarized - Light
  // More theme here: http://softwaremaniacs.org/media/soft/highlight/test.html
